{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.4.1",
      "destination": "wwwroot/lib/bootstrap/dist"
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/dist",
      "files": ["jquery.js", "jquery.min.js"]
    },
    {
      "library": "jquery-validate@1.19.0",
      "destination": "wwwroot/lib/jquery-validation/dist",
      "files": ["jquery.validate.js","jquery.validate.min.js","additional-methods.js","additional-methods.min.js"]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/dist",
      "files": ["jquery-validation-unobtrusive.js","jquery-validation-unobtrusive.min.js"]
    }
  ]
}